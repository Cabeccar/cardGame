{
  "name": "babylongame",
  "version": "1.0.0",
  "description": "A game with Babylon.js",
  "main": "index.ts",
  "scripts": {
    "start": "webpack-dev-server --inline --mode=development",
    "build": "NODE_ENV=production webpack --mode=production",
    "crb": "npm run build && cd cordova/babylonGame && cordova run browser",
