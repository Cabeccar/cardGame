    "url": "https://github.com/GBatault/babylonGame/issues"
  },
  "homepage": "https://github.com/GBatault/babylonGame#readme",
  "dependencies": {
    "babylonjs": "^3.2.0-rc.3",
    "babylonjs-gui": "^3.2.0"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^5.0.0",
    "cordova": "^8.0.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "typescript": "^2.8.3",
